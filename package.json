{
  "name": "my-fin-control-backend",
  "version": "1.0.0",
  "description": "Backend API para MyFinControl - Sistema de controle financeiro e listas de compras",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "set NODE_ENV=production && node dist/server.js",
    "dev": "set NODE_ENV=development && ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:prod": "set NODE_ENV=production && ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:dev": "set NODE_ENV=development && jest",
    "test:prod": "set NODE_ENV=production && jest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "db:reset:dev": "set NODE_ENV=development && node -e \"require('./dist/config/database.js').databaseManager\"",
    "db:reset:prod": "set NODE_ENV=production && node -e \"require('./dist/config/database.js').databaseManager\""
  },
  "keywords": [
    "finance",
    "shopping",
    "api",
    "rest",
    "typescript",
    "express"
  ],
  "author": "MyFinControl",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^8.9.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
